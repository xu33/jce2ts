	struct NewsSIndexDataNew
    {
        1 optional short shtMarket;
        2 optional string sCode;
	    3 optional string sStockName;
	    4 optional double dChangeRate;       //小数, 涨跌幅
	    5 optional string sSignalLastTime;   //信号持续时间描述
        6 optional string sChoseReason;      //"入选涨幅", "次日涨幅"等
        7 optional string sChoseTime;        //入选时间串
        8 optional int iLimitUpType;      //涨停类型
        9 optional string sUrl;              //股票池"小股票块"的跳转url
    };

	//统一存放各种牛逼的复杂的自产内容
    struct NewsRecomTgLive {
        0 optional string sImgUrl;   //投顾老师头像url
        1 optional string sLiveRoomName; //直播房间名称
        2 optional string sLiveTitle;     //直播主题
        3 optional int iJoinNum;          //参加人数
        4 optional string sLiveUrl;       //跳转链接
        5 optional int iStatus; //0-结束， 1-直播中
        6 optional int iCommentNum;   //评论人数
        7 optional string sNewMessage;  //新的消息
        8 optional bool bHasImage;     //是否含有图片
		9 optional string	sLiveId;//直播室ID	
    };

	struct NewsContentColumn
	{
		0 optional string	summary;
		1 optional string	title;
		2 optional string	toUrl;
	};
	//视频信息
	struct NewsVedioInfo
	{
		0 optional int		multiMediaStyle;	//对接E_Multimedia_Style结构体。0表示视频展示样式大；1表示视频展示样式小；2表示关注内容更新
		1 optional int		multiMediaStatus;	//对接E_Multimedia_status结构体。0表示视频直播状态；1表示视频直播预告；2表示视频转码；3表示视频回放
		2 optional string	fileUrl;			// 路径或列表页图片或用户头像
		3 optional string	videoUrl;			//列表页视频链接
		4 optional string	title;				//视频标题
		5 optional string	teacherName;		//视频老师名字
		6 optional string	toUrl;				// 跳转的url
		7 optional int		type;				//对接E_Multimedia_Type。0表示图片；2表示文件；3表示视频；4表示广告
		8 optional int		playCount;			//播放次数
		9 optional string	vedioId;			//视频ID
		10 optional int		startTime;			//开始时间
		11 optional int		endTime;			//结束时间
	};

	struct  NewsTagInfo
	{
		0 require  int		type;			// 标签类型 取值E_Tage_Type
		1 optional int		value;			// 标签的值
		2 optional string   tittle;			// 语句
	};
	
	//动态信息
	struct OneDynamicData
	{
		0 optional int				id;			// 动态数据的id号
		1 optional string			stockName;	// 股票名
		2 optional string			stockCode;	// 股票代码
		3 optional int				market;		// 市场代码
		4 optional string			message;	// 动态摘要信息
		5 optional string			dataName;	// 数据类型名
		6 optional NewsTagInfo		tag;		// 资讯标签
		7 optional long				createTime;	// 消息发生的时间
	};

	struct DynamicData
	{
		0 optional int	dataCount;	// 新数据条数，不一定等于vDynamicData.size()
		1 optional vector<OneDynamicData> vDynamicData;	// 
	};

    struct NewsRecomAssemblyData {
        0 optional vector<NewsRecomTgLive>		vTgLive;
		1 optional vector<NewsContentColumn>	vContentColumn;
		2 optional vector<NewsVedioInfo>		vVedioInfo;
		3 optional DynamicData					dynamicData;
    };
